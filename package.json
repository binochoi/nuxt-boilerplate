{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "run-s prepare",
    "prepare": "nuxt prepare",
    "build": "run-s build.*",
    "build.only": "NITRO_PRESET=cloudflare_module nuxt build",
    "dev": "nuxt dev --dotenv .dev.vars",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "db:generate": "rimraf .cache/.migrations && drizzle-kit generate --config=./drizzle.config.ts",
    "db:migrate": "run-s db:migrate.*",
    "db:migrate.migrate": "drizzle-kit migrate --config=./drizzle.config.ts",
    "db:migrate.push": "drizzle-kit push --config=./drizzle.config.ts"
  },
  "dependencies": {
    "@iconify/vue": "^4.2.0",
    "@ionic/vue": "^8.4.1",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@pinia/nuxt": "^0.9.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@vueuse/core": "^12.0.0",
    "@vueuse/nuxt": "^12.0.0",
    "@vueuse/router": "^12.0.0",
    "better-auth": "^1.0.22",
    "delay": "^6.0.0",
    "drizzle-kit": "^0.30.1",
    "drizzle-orm": "^0.38.2",
    "nitro-cloudflare-dev": "^0.2.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.14.1592",
    "pinia": "^2.3.0",
    "postgres": "^3.4.5",
    "sass": "^1.83.0",
    "tailwindcss": "^3.4.7",
    "tailwindcss-animate": "^1.0.7",
    "trpc-nuxt": "^0.10.22",
    "valibot": "1.0.0-beta.9",
    "vue": "latest",
    "vue-router": "latest",
    "wrangler": "^3.97.0"
  },
  "devDependencies": {
    "@binochoi/eslint-config-typescript": "^0.0.25",
    "@binochoi/eslint-config-vue3": "^0.0.14",
    "@eslint/eslintrc": "^3.2.0",
    "@types/eslint__eslintrc": "^2.1.2",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0"
  },
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
